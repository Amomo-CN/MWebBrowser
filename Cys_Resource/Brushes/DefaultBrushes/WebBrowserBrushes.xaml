<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <SolidColorBrush x:Key="WebBrowserBrushes.TabHeaderBackground" Color="#1B1B1B"/>
    <SolidColorBrush x:Key="WebBrowserBrushes.TabHeaderIsSelectedBackground" Color="#3B3B3B"/>
    <SolidColorBrush x:Key="WebBrowserBrushes.TabHeaderIsMouseOverBackground" Color="#272727"/>

    <SolidColorBrush x:Key="WebBrowserBrushes.TabCloseIsMouseOverBackground" Color="#4A4A4A"/>
    <SolidColorBrush x:Key="WebBrowserBrushes.TabCloseIsPressBackground" Color="#636363"/>

    <SolidColorBrush x:Key="WebBrowserBrushes.SearchBackgroundColor" Color="#2B2B2B"/>
